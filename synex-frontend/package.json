{
  "name": "synex",
  "version": "0.0.2",
  "description": "AI-powered CLI for developers",
  "bin": {
    "synex": "./bin/synex.js"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch --onSuccess 'node bin/synex.js'",
    "build": "tsup src/tui-index.tsx --format esm --out-dir bin --clean && mv bin/tui-index.js bin/synex.js",
    "start": "node bin/synex.js"
  },
  "type": "module",
  "dependencies": {
    "@types/react": "^19.1.10",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "globby": "^13.2.2",
    "ink": "^6.1.1",
    "ink-text-input": "^6.0.0",
    "ora": "^7.0.1",
    "react": "^19.1.1",
    "string-width": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
